{
  "name": "platzibadges",
  "version": 2,
  "builds": [{ "src": "index.js", "use": "@now/node" }],
  "routes": [{ "src": "/(.*)", "dest": "/index.js" }],
  "env": {
    "DB_USER": "@db-user",
    "DB_PASSWORD": "@badges-db-password",
    "DB_HOST": "@badges-db-host",
    "DB_NAME": "@badges-db-name"
  }
}
